{
	"preInstallScripts" : [
		"ls /cache", 
		"touch /cache/iets",
		"mkdir -p /cache/wercker/apt",
		"sudo rm -fr /var/lib/apt/list",
		"sudo ln -s /cache/werkcer/apt /var/lib/apt/list",
		"mkdir -p /cache/wercker/home",
		"sudo ln -s /cache/werkcer/home /home/ubuntu",
		"sudo chown ubuntu /home/ubuntu",
		"sudo apt-get update",
		"sudo apt-get install openjdk-6-jre"
		]
}

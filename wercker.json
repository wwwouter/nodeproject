{
	"preInstallScripts" : [
		"ls /cache", 
		"touch /cache/iets",
		"mkdir -p /cache/wercker/apt-list",
		"mkdir -p /cache/wercker/apt-cache",
		"sudo rm -fr /var/lib/apt/lists",
		"sudo rm -fr /var/cache/apt",
		"sudo ln -s /cache/wercker/apt-list/ /var/lib/apt/lists",
		"sudo ln -s /cache/wercker/apt-cache/ /var/cache/apt",
		"mkdir -p /cache/wercker/home",
		"sudo ln -s /cache/wercker/home/ /home/ubuntu",
		"sudo chown ubuntu /home/ubuntu",
		"echo sudo apt-get update",
		"sudo apt-get install openjdk-6-jre"
		]
}
